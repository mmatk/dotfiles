alias httpdump='sudo tcpdump -i eth0 -n -s 0 -w - | grep -a -o -E "Host\: .*|GET \/.*"'
alias c='clear'
alias cdc='cd;clear'
alias s='sudo'
alias v='vim'
alias sv='sudo vim'
alias ls='ls -F --color'
alias lsd='ll -d */'
alias ll='ls -lhF --color'
alias lss='ls -lSh'
alias all='ls -AlhiF --color'
alias h='history | grep $1'
alias x='startx'
alias q='exit'
alias ZZ='exit'
alias r='cd;c'
alias shutdown='sudo shutdown -h now'
alias reboot='sudo reboot'
alias emerge='sudo emerge'
alias ebuild='sudo ebuild'
alias sc='screen'
alias scr='screen -r'
